@page
@{
    Layout = "~/Pages/Shared/_Layout.cshtml";
}
<div class="app-container" dir="ltr">
    <div class="container-fluid px-0 main-content-page h-100">
        <partial name="Organization_Tabs" />
        <div class="aos-init aos-animate">
            <div class="child-content nav-active-top" id="org-structure">
                <ul class="topnav-child-1">
                    <li class="nav-active-top"><a href="/organization/onboarding/candidate_view"> Candidates</a> </li>
                    <li><a href="/organization/onboarding/outsourcing_employee"> Outsources</a> </li>
                </ul>
                <div class="container-fluid p-3">
                    <div class="grand-child nav-active-top">
                        <div id="CandidateDetailPage" class="ng-star-inserted">
                            <div class="page-title-h">
                                <div class="d-flex justify-content-start align-items-center">
                                    <mat-icon role="img" class="mat-icon notranslate text-muted material-icons mat-icon-no-color" style="cursor: pointer;">chevron_left </mat-icon>
                                    <h4 class="card-title m-0 card-heading">Candidate Details</h4>
                                </div>
                            </div>
                            <mat-card class="mat-card mat-focus-indicator b-shadow-none border-0">
                                <div class="container-fluid py-0">
                                    <div class="d-flex justify-content-end tabcol mb-1">
                                        <div style="position: relative;">
                                            <input type="text" class="grid-search-box" placeholder="Search" fdprocessedid="xdmbl">
                                            <mat-icon role="img" class="mat-icon notranslate search-icon material-icons mat-icon-no-color">search</mat-icon>
                                        </div>
                                        <button type="button" class="mat-ripple p-element rd-round-button ng-star-inserted" tabindex="0"><img src="/images/upload_file.svg"></button>
                                        <div class="btn-group">
                                            <button type="button" class="mat-ripple p-element rd-round-button ng-star-inserted dropdown-toggle" data-bs-toggle="dropdown" aria-expanded="false"><img src="/images/add.svg"></button>
                                            <ul class="dropdown-menu">
                                                <li><a onclick="addCandidate()">New Candidate</a></li>
                                                <li><a onclick="addBulkCandidateViaMail()">Bulk Candidate via mail</a></li>
                                            </ul>
                                        </div>
                                        <button type="button" ptooltip="Download" tooltipposition="top" class="mat-ripple p-element mat-menu-trigger rd-round-button"><img src="/images/download.svg"></button>
                                    </div>
                                    <div class="col-12 px-0">
                                        <div class="ng-star-inserted">
                                            <div class="example-table-container rd-table-wrapper">
                                                <table class="mat-table cdk-table">
                                                    <thead role="rowgroup">
                                                        <tr class="mat-header-row cdk-header-row ng-star-inserted">
                                                            <th class="mat-header-cell cdk-header-cell">Name</th>
                                                            <th class="mat-header-cell cdk-header-cell">Mail</th>
                                                            <th class="mat-header-cell cdk-header-cell text-center">Onboarding Status</th>
                                                            <th class="mat-header-cell cdk-header-cell text-center">Trigger Email</th>
                                                            <th class="mat-header-cell cdk-header-cell text-center">Migration</th>
                                                            <th class="mat-header-cell cdk-header-cell"> Actions </th>
                                                        </tr>
                                                    </thead>
                                                    <tbody role="rowgroup">
                                                        <tr class="mat-row cdk-row ng-star-inserted">
                                                            <td class="mat-cell cdk-cell">Tom  Holland</td>
                                                            <td class="mat-cell cdk-cell">tom.holland@clientdbin.com</td>
                                                            <td class="mat-cell cdk-cel text-center"><span class="badge rounded-pill text-bg-warning">In Progress</span></td>
                                                            <td class="mat-cell cdk-cel text-center"><img src="/images/mail.svg" alt="Mail" class="img-fluid" width="26" /></td>
                                                            <td class="mat-cell cdk-cell text-center">-</td>
                                                            <td class="mat-cell cdk-cell">
                                                                <img src="/images/edit1.svg" alt="Edit" title="Edit" width="16" class="img-fluid me-2">
                                                                <img src="/images/trash.svg" alt="Delete" title="Delete" width="16" class="img-fluid">
                                                            </td>
                                                        </tr>
                                                        <tr class="mat-row cdk-row ng-star-inserted">
                                                            <td class="mat-cell cdk-cell">Tom  Holland</td>
                                                            <td class="mat-cell cdk-cell">tom.holland@clientdbin.com</td>
                                                            <td class="mat-cell cdk-cel text-center"><span class="badge rounded-pill text-bg-warning">In Progress</span></td>
                                                            <td class="mat-cell cdk-cel text-center"><img src="/images/mail.svg" alt="Mail" class="img-fluid" width="26" /></td>
                                                            <td class="mat-cell cdk-cell text-center">-</td>
                                                            <td class="mat-cell cdk-cell">
                                                                <img src="/images/edit1.svg" alt="Edit" title="Edit" width="16px" class="img-fluid me-2">
                                                                <img src="/images/trash.svg" alt="Delete" title="Delete" width="16px" class="img-fluid">
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="mat-paginator">
                                        <div class="mat-paginator-outer-container">
                                            <div class="mat-paginator-container">
                                                <div class="mat-paginator-page-size ng-star-inserted">
                                                    <div class="mat-paginator-page-size-label"> Items per page: </div>
                                                    <mat-form-field class="mat-form-field mat-paginator-page-size-select mat-primary mat-form-field-type-mat-select mat-form-field-appearance-legacy mat-form-field-can-float mat-form-field-should-float mat-form-field-hide-placeholder ng-star-inserted">
                                                        <div class="mat-form-field-wrapper">
                                                            <div class="mat-form-field-flex">

                                                                <div class="mat-form-field-infix">
                                                                    <mat-select role="combobox" aria-autocomplete="none" aria-haspopup="true" class="mat-select ng-star-inserted" id="mat-select-38" tabindex="0" aria-expanded="false" aria-label="Items per page:" aria-required="false" aria-disabled="false" aria-invalid="false">
                                                                        <div class="mat-select-trigger">
                                                                            <div class="mat-select-value" id="mat-select-value-39">

                                                                                <span class="mat-select-value-text ng-star-inserted">
                                                                                    <span class="mat-select-min-line ng-star-inserted">12</span>
                                                                                </span>

                                                                            </div>
                                                                            <div class="mat-select-arrow-wrapper">
                                                                                <div class="mat-select-arrow"></div>
                                                                            </div>
                                                                        </div>

                                                                    </mat-select>
                                                                    <span class="mat-form-field-label-wrapper">

                                                                    </span>
                                                                </div>

                                                            </div>
                                                            <div class="mat-form-field-underline ng-star-inserted"><span class="mat-form-field-ripple"></span></div>

                                                            <div class="mat-form-field-subscript-wrapper">

                                                                <div class="mat-form-field-hint-wrapper ng-trigger ng-trigger-transitionMessages ng-star-inserted" style="opacity: 1; transform: translateY(0%);">

                                                                    <div class="mat-form-field-hint-spacer"></div>
                                                                </div>

                                                            </div>
                                                        </div>
                                                    </mat-form-field>

                                                </div>

                                                <div class="mat-paginator-range-actions">
                                                    <div class="mat-paginator-range-label"> 1 - 1 of 1 </div>

                                                    <button type="button" class="mat-focus-indicator mat-tooltip-trigger mat-paginator-navigation-previous mat-icon-button mat-button-base mat-button-disabled" aria-label="Previous page" disabled="true">
                                                        <span class="mat-button-wrapper">
                                                            <svg viewBox="0 0 24 24" focusable="false" class="mat-paginator-icon">
                                                                <path d="M15.41 7.41L14 6l-6 6 6 6 1.41-1.41L10.83 12z"></path>
                                                            </svg>
                                                        </span>
                                                        <span matripple="" class="mat-ripple mat-button-ripple mat-button-ripple-round"></span><span class="mat-button-focus-overlay"></span>
                                                    </button>

                                                    <button type="button" class="mat-focus-indicator mat-tooltip-trigger mat-paginator-navigation-next mat-icon-button mat-button-base mat-button-disabled" aria-label="Next page" disabled="true">
                                                        <span class="mat-button-wrapper">
                                                            <svg viewBox="0 0 24 24" focusable="false" class="mat-paginator-icon">
                                                                <path d="M10 6L8.59 7.41 13.17 12l-4.58 4.59L10 18l6-6z"></path>
                                                            </svg>
                                                        </span>
                                                        <span matripple="" class="mat-ripple mat-button-ripple mat-button-ripple-round"></span><span class="mat-button-focus-overlay"></span>
                                                    </button>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </mat-card>
                        </div>
                        <div id="addCandidateDetails" class="ng-star-inserted">
                            <div class="page-title-h">
                                <div class="d-flex justify-content-start align-items-center">
                                    <mat-icon role="img" class="mat-icon notranslate text-muted material-icons mat-icon-no-color" style="cursor: pointer;" onclick="backToCanDetails()">chevron_left </mat-icon>
                                    <h4 class="card-title m-0 card-heading">Add Candidate</h4>
                                </div>
                            </div>
                            <mat-card class="mat-card mat-focus-indicator b-shadow-none border-0">
                                <div class="mainContentHeightOrg2">
                                    <div class="container-fluid">
                                        <form novalidate="" class="ng-untouched ng-invalid ng-dirty">
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">Title</label>
                                                        <select class="form-select" placeholder="Select Title">
                                                            <option selected>Select Employee Type</option>
                                                            <option value="1">1</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">First Name*</label>
                                                        <input type="text" class="form-control" placeholder="Enter First Name" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">Last Name*</label>
                                                        <input type="text" class="form-control" placeholder="Enter Last Name" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">Gender</label>
                                                        <select class="form-select" placeholder="Select Gender">
                                                            <option selected>Select Title</option>
                                                            <option value="1">1</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">Personal Email*</label>
                                                        <input type="email" class="form-control" placeholder="Enter Personal Email" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">Personal Mobile</label>
                                                        <input type="text" class="form-control" placeholder="Enter Personal Mobile" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">Date of Birth*</label>
                                                        <input type="date" class="form-control" placeholder="Date of Birth" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">Marital Status</label>
                                                        <select class="form-select" placeholder="Select Marital Status">
                                                            <option selected>Select Employee Status</option>
                                                            <option value="1">1</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">
                                                            Work Location *
                                                        </label>
                                                        <select class="form-select" placeholder="Select Work Location">
                                                            <option selected>Select Country</option>
                                                            <option value="1">1</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">Onboarding Groups</label>
                                                        <select class="form-select" placeholder="Select Onboarding Groups">
                                                            <option selected>Select State</option>
                                                            <option value="1">1</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">Passport Number</label>
                                                        <input type="text" class="form-control" placeholder="i.e.JXXXXXXXXXX" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">Date of Join</label>
                                                        <input type="date" class="form-control" placeholder="Date of Birth" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="separator my-2"><span class="border p-1 small rounded head fw-bold head px-2">Address</span></div>
                                            <div class="row align-items-center">
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">Present Address*</label>
                                                        <input type="text" class="form-control" placeholder="Enter Present Address" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4 my-2">
                                                    <div>
                                                        <label class="rd-form-select-label mb-2 me-2 text-muted"><input type="checkbox" id="1" class="form-check-input" name="checkBoxName">  <span> Onboard employee with probation  </span></label>
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">Permanent Address</label>
                                                        <input type="text" class="form-control" placeholder="Enter Permanent Address" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">Country*</label>
                                                        <select class="form-select" placeholder="Select Country">
                                                            <option selected>Select City</option>
                                                            <option value="1">1</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">State*</label>
                                                        <select class="form-select" placeholder="Select State">
                                                            <option selected>Select City</option>
                                                            <option value="1">1</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">City</label>
                                                        <select class="form-select" placeholder="Select City">
                                                            <option selected>Select City</option>
                                                            <option value="1">1</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">Zipcode*</label>
                                                        <input type="text" class="form-control" placeholder="Enter Zipcode" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="rd-form-select my-1">
                                                        <label class="rd-form-select-label mb-1">Nationality</label>
                                                        <input type="text" class="form-control" placeholder="Enter Nationality" />
                                                    </div>
                                                </div>
                                                <div class="text-end">
                                                    <button type="submit" color="primary" class="rd-btn-primary-sm">
                                                        Submit
                                                    </button>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </mat-card>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


