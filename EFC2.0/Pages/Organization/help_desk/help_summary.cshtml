@page
@{
    Layout = "~/Pages/Shared/_Layout.cshtml";
}
<div class="app-container" dir="ltr">
    <div class="container-fluid px-0 main-content-page h-100">
        <partial name="Organization_Tabs" />
        <div class="aos-init aos-animate">
            <div class="child-content nav-active-top" id="summary">
                <ul class="topnav-child-1">
                    <li class="nav-active-top"><a href="/organization/help_desk/help_summary"> Summary</a> </li>
                    <li><a href="/organization/help_desk/question_category_view"> Categories</a> </li>
                    <li><a href="/organization/help_desk/question_view"> Grievance</a> </li>
                    <li><a href="/organization/help_desk/help_tickets"> Tickets</a></li>
                    <li><a href="/organization/help_desk/close_reason"> Close Reasons</a></li>
                    <li><a href="/organization/help_desk/KnowledgeBase"> Knowledge Base</a></li>
                    <li><a href="/organization/help_desk/sla"> SLA Config</a></li>
                    <li><a href="/organization/help_desk/team"> Help-Desk Team</a></li>
                </ul>
                <div class="container-fluid p-3">
                    <div class="grand-child nav-active-top">
                        <div class="ng-star-inserted">
                            <div class="row">
                                <div class="col-md-2 tabcol mb-3">
                                    <label class="rd-form-select-label mb-1">Grievance Category</label>
                                    <select class="form-select">
                                        <option value="Laptop" selected>Laptop</option>
                                        <option value="Mr.">Mr.</option>
                                        <option value="Ms.">Ms.</option>
                                        <option value="Mrs.">Mrs.</option>
                                    </select>
                                </div>
                            </div>
                            <div class="d-flex justify-content-between mb-3">
                                <div class="col-md-2 tabcols">
                                    <div class="rd-card rd-p-1">
                                        <div class="p-1">
                                            <div class="fs-14 text-muted">Total Ticket Count</div>
                                            <h3 class="pt-2">0</h3>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-2 tabcols">
                                    <div class="rd-card rd-p-1">
                                        <div class="p-1">
                                            <div class="fs-14 text-muted">Ticket Created Today</div>
                                            <h3 class="pt-2">0</h3>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-2 tabcols">
                                    <div class="rd-card rd-p-1">
                                        <div class="p-1">
                                            <div class="fs-14 text-muted">Ticket Closed Today</div>
                                            <h3 class="pt-2">0</h3>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-2 tabcols">
                                    <div class="rd-card rd-p-1">
                                        <div class="p-1">
                                            <div class="fs-14 text-muted">Open Tickets Count</div>
                                            <h3 class="pt-2">0</h3>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-2 tabcols">
                                    <div class="rd-card rd-p-1">
                                        <div class="p-1">
                                            <div class="fs-14 text-muted">Closed Ticket Count</div>
                                            <h3 class="pt-2">0</h3>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6 tabcol mb-3">
                                    <mat-card class="mat-card mat-focus-indicator b-shadow-none border-0">
                                        <div class="mainContentHeightOrg2">
                                            <h4 class="head">Tickets Data</h4>
                                            <div class="row">
                                                <div class="col-md-4 tabcol mb-3">
                                                    <label class="rd-form-select-label mb-1">Date Range</label>
                                                    <select class="form-select">
                                                        <option value="Laptop" selected>Laptop</option>
                                                        <option value="Mr.">Mr.</option>
                                                        <option value="Ms.">Ms.</option>
                                                        <option value="Mrs.">Mrs.</option>
                                                    </select>
                                                </div>
                                                <div class="col-md-2 d-flex align-items-end tab-col-width2 mb-3"><button type="button" class="mx-2 rd-btn-tertairy-sm rd-text-secondary-1">Reset</button><button type="button" class="mx-2 rd-btn-primary-sm rd-text-primary-1">Submit</button></div>
                                            </div>
                                            <div id="ticketData" style="height: 280px; width: 100%;"></div>
                                        </div>
                                    </mat-card>
                                </div>
                                <div class="col-md-6 tabcol mb-3">
                                    <mat-card class="mat-card mat-focus-indicator b-shadow-none border-0">
                                        <div class="mainContentHeightOrg2">
                                            <h4 class="head">Yearly Tickets Data</h4>
                                            <div class="row align-items-end">
                                                <div class="col-md-4 tabcol mb-3">
                                                    <label class="rd-form-select-label mb-1">Year</label>
                                                    <select class="form-select">
                                                        <option value="Laptop" selected>Laptop</option>
                                                        <option value="Mr.">Mr.</option>
                                                        <option value="Ms.">Ms.</option>
                                                        <option value="Mrs.">Mrs.</option>
                                                    </select>
                                                </div>
                                                <div class="col-md-4 tabcol mb-3">
                                                    <label class="rd-form-select-label mb-1">Status</label>
                                                    <select class="form-select">
                                                        <option value="Laptop" selected>Laptop</option>
                                                        <option value="Mr.">Mr.</option>
                                                        <option value="Ms.">Ms.</option>
                                                        <option value="Mrs.">Mrs.</option>
                                                    </select>
                                                </div>
                                                <div class="col-md-3 tabcol mb-3">
                                                    <button type="button" class="mx-2 rd-btn-primary-sm rd-text-primary-1">Search</button>
                                                </div>
                                            </div>
                                            <div id="yearlyTicketData" style="height: 280px; width: 100%;"></div>
                                        </div>
                                    </mat-card>
                                </div>
                                <div class="col-md-6 tabcol mb-3">
                                    <mat-card class="mat-card mat-focus-indicator b-shadow-none border-0">
                                        <div class="mainContentHeightOrg2">
                                            <h4 class="head">Priority</h4>
                                            <div id="priorityData" style="height: 280px; width: 100%;"></div>
                                        </div>
                                    </mat-card>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="https://cdn.canvasjs.com/canvasjs.min.js"></script>
    <script>
    window.onload = function () {

        var chart = new CanvasJS.Chart("ticketData", {
	animationEnabled: true,
	theme: "light2", // "light1", "light2", "dark1", "dark2"
	axisY: {
		title: "",
		suffix: ""
	},
	axisX: {
		title: ""
	},
	data: [{
		type: "column",
		yValueFormatString: "#,##0.0#"%"",
		dataPoints: [
			{ label: "India", y: 7.1 },	
			{ label: "China", y: 6.70 },	
			{ label: "Indonesia", y: 5.00 },
			{ label: "Australia", y: 2.50 },	
			{ label: "Mexico", y: 2.30 },
			{ label: "UK", y: 1.80 },
			{ label: "United States", y: 1.60 },
			{ label: "Japan", y: 1.60 }
			
		]
	}]
});
chart.render();

    }
</script>